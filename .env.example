# Example environment for local development
NODE_ENV=development
PORT=3000
LOGGER_LEVEL=log

# Database (Postgres)
DB_TYPE=postgres
DB_HOST=postgres
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=postgres
DB_NAME=nestjs_app
DB_SYNCHRONIZE=true
DB_LOGGING=false

# Redis
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# Security
ALLOWED_APPLICATIONS=medusajs-storefront,medusajs-admin,mobile-app
ADMIN_API_KEY=otp-admin-key-CHANGE-THIS-TO-A-SECURE-RANDOM-STRING
